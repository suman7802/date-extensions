{
  "manifest_version": 2,
  "name": "Nepali Date Extension",
  "version": "1.0",
  "description": "An extension to display Nepali date",
  "permissions": ["activeTab", "https://jsonplaceholder.typicode.com/"],
  "browser_action": {
    "default_popup": "index.html",
    "default_icon": {
      "16": "images/icon16.png",
      "48": "images/icon48.png",
      "128": "images/icon128.png"
    }
  }
}
